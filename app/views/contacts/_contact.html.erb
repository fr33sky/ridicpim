<tr id="<%= contact.id %>">
  <td>
    <%= link_to fa_icon("search"), contact %>
    <%= link_to fa_icon("pencil"), edit_contact_path(contact), remote: true %>
    <%= link_to fa_icon("trash" ), contact, method: :delete, data: { confirm: "Are you sure?" }, remote: true %>
  </td>
  <td><%= contact.name %></td>
  <td><%= contact.address %></td>
  <td><%= contact.city %></td>
  <td><%= contact.state %></td>
  <td><%= contact.postal_code %></td>
  <td><%= contact.country %></td>
  <td><%= contact.email_address %></td>
  <td><%= contact.phone_number %></td>
  <td><%= contact.qbo_id %></td>
</tr>
